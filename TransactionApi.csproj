    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>581faa01-ef5f-4f11-adf8-6c918ae076f7</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="TransactionApi.Tests\**\*" />
